{
  "name": "rococo-wococo-bridged-parachains",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Ignacio Palacios",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./bin/index.js",
    "dev": "ts-node ./src/index.ts",
    "dmp": "ts-node ./src/index.ts dmp",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'tests/*/*.ts'",
    "test:xcm": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'tests/xcm/*.ts'",
    "test:xcm:limited-teleport-asset": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'tests/xcm/teleportAssets.ts'",
    "test:xcm:transact": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'tests/xcm/transact.ts'",
    "test:assets": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 100000 -r ts-node/register 'tests/assets/*.ts'",
    "dev:register-parachains": "ts-node ./src/registerParachains.ts",
    "register-parachains": "node ./bin/src/registerParachains.js",
    "dev:init-chains": "ts-node ./src/initChains.ts",
    "init-chains": "node ./bin/src/initChains.js",
    "dev:assets:create": "ts-node ./src/assetsCreate.ts",
    "assets:create": "node ./bin/src/assetCreate.js",
    "dev:assets:set-metadata": "ts-node ./src//assetsSetMetadata.ts",
    "assets:set-metadata": "node ./bin/src/assetsSetMetadata.js",
    "dev:assets:mint": "ts-node ./src//assetsMint.ts",
    "assets:mint": "node ./bin/src/assetsMint.js"
  },
  "dependencies": {
    "@polkadot/api": "^6.6.1",
    "@polkadot/ui-keyring": "^0.85.3",
    "@types/node": "^16.7.10",
    "bn.js": "^5.2.0",
    "command-line-args": "^5.2.0",
    "dotenv": "^10.0.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "chai-bn": "^0.3.0",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0"
  }
}
